create database investimentos;
use investimentos;

create table Usuarios(
cpf_cnpj char(14) not null,
nome varchar(70) not null,
dataNasc date not null,
telefone char(16) not null,
senha varchar(45) not null,
primary key(cpf_cnpj)
);

create table Movimentacoes(
cod int auto_increment,
nome_atk char(14) not null,
nome_moeda char(14) not null,
capital float not null,
meses int not null,
montante float not null,
primary key (cod)
);

create table Movimentacoes_n(
cod_n int auto_increment,
nome_atk_n char(14) not null,
nome_moeda_n char(14) not null,
capital_n float not null,
montante_n float not null,
primary key (cod_n)
);

select * from Usuarios;
select * from Movimentacoes_n;

drop table Usuarios;
drop table Movimentacoes_n;

show tables;